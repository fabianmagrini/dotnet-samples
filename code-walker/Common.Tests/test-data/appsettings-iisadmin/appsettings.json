{
    "host_id": "",
  
    "host_name": "My instance of the IIS Administration API",
  
    "logging": {
      "enabled": true,
      "file_name": "log-{Date}.txt",
      "min_level": "Error",
      "path": null
    },
  
    "auditing": {
      "enabled": true,
      "file_name": "audit-{Date}.txt",
      "path": null
    },
  
    "security": {
      "require_windows_authentication": true,
      "users": {
        "administrators": [
        ],
        "owners": [
        ]
      },
      "access_policy": {
        "api": {
          "users": "administrators",
          "access_key": true
        },
        "api_keys": {
          "users": "administrators",
          "access_key": false
        },
        "system": {
          "users": "owners",
          "access_key": true
        }
      }
    },
  
    "cors": {
      "rules": [
        {
          "origin": "https://contoso.com",
          "allow": true
        }
      ]
    },
  
    "files": {
      "locations": [
        {
          "alias": "inetpub",
          "path": "%systemdrive%\\inetpub",
          "claims": [
            "read"
          ]
        }
      ]
    }
  }